{
  "entrypoint": "index.html",
  "shell": "src/yta-shell.html",
  "fragments": [
    "src/yta-timetable-page.html",
    "src/yta-teachers-page.html",
    "src/yta-calendar-page.html",
    "src/yta-settings.html",
    "src/yta-onboarding.html",
    "src/yta-add-dialog.html",
    "src/yta-feedback-dialog.html",
    "src/yta-setup-dialog.html"
  ],
  "sources": [
   "src/*",
   "images/**/*",
   "bower.json"
  ],
  "extraDependencies": [
    "sw-runner.js",
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.min.js",
    "src/polyfills/array-from-polyfill.js",
    "src/polyfills/request-idle-callback.js",
    "privacy-policy.html"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "name": "bundled",
    "bundle": true,
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  },{
    "name": "unbundled",
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  }]
}